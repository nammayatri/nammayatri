# access tokens with different roles
@customer = {{bpp-dashboard/token/NAMMA_YATRI_PARTNER/customer}}
@driver = {{bpp-dashboard/token/NAMMA_YATRI_PARTNER/driver}}
@juspay_ops = {{bpp-dashboard/token/NAMMA_YATRI_PARTNER/juspay_ops}}
@juspay_admin = {{bpp-dashboard/token/NAMMA_YATRI_PARTNER/juspay_admin}}
@customer_service = {{bpp-dashboard/token/NAMMA_YATRI_PARTNER/customer_service}}
@operator = favorit-operator-token0-000000000000

# @name healthcheck
GET {{bpp-dashboard-host}}

###

# @name documentsList
# DocumentsListAPI

@driverId1 = favorit-auto1-0000000000000000000000

GET {{bpp-dashboard-host}}/bpp/driver-offer/NAMMA_YATRI_PARTNER/KOCHI/driver/{driverId1}/documents/list
token: {{customer_service}}

###

# @name postDriverRegistrationMediaFileDocumentUploadLink

POST {{bpp-dashboard-host}}/bpp/driver-offer/NAMMA_YATRI_PARTNER/KOCHI/driver/mediaFileDocument/uploadLink
token: {{operator}}
content-type: application/json

{ "mediaFileDocumentType": "VehicleVideo",
  "fileType": "Video",
  "reqContentType": "video/mp4",
  "rcNumber": "WB1234567"
}

###

# @name postDriverRegistrationMediaFileDocumentDownloadLink

GET {{bpp-dashboard-host}}/bpp/driver-offer/NAMMA_YATRI_PARTNER/KOCHI/driver/mediaFileDocument/downloadLink
  ?mediaFileDocumentType="VehicleVideo"
  &rcNumber=WB1234567
token: {{operator}}
content-type: application/json

###

# @name postDriverRegistrationMediaFileDocumentConfirm

@mediaFileDocumentId = {{postDriverRegistrationMediaFileDocumentDownloadLink.response.body.mediaFileDocumentId}}

POST {{bpp-dashboard-host}}/bpp/driver-offer/NAMMA_YATRI_PARTNER/KOCHI/driver/mediaFileDocument/confirm
token: {{operator}}
content-type: application/json

{ "mediaFileDocumentId": "{{mediaFileDocumentId}}"
}

###

# @name postDriverRegistrationMediaFileDocumentDelete

POST {{bpp-dashboard-host}}/bpp/driver-offer/NAMMA_YATRI_PARTNER/KOCHI/driver/mediaFileDocument/delete
token: {{operator}}
content-type: application/json

{ "mediaFileDocumentId": "{{mediaFileDocumentId}}"
}
